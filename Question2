#input phase    sofia lopez 1/26/26 
itemcd = input("Enter item (A or B): ")
quantit = int(input("Enter quantity: "))

#process phase 
if itemcd == "A":
    unitprc = 10.00
else:
    unitprc = 20.00

extprc = quantit * unitprc

#output phase 
print("Item:", itemcd)
print("Unit Price:", unitprc)
print("Extended Price:", extprc)
